{
    "name":"sdpController",
    "vendor":"JAS",
    "version":"1.0.0",
    "lifecycle_event":[
        {
            "event":"INSTANTIATE",
            "lifecycle_events":[
                "setup.sh"
            ]
        },
       	{
            "event":"START",
            "lifecycle_events":[
                "run.sh"
            ]
        }
    ],
    "vdu":[
        {
            "vm_image":[],
            "scale_in_out":2,
            "vnfc":[
                {
                    "connection_point":[
                        {
                            "virtual_link_reference":"new-network-jas"
                        }
                    ]
                }
            ],
            "vimInstanceName":["vim-instance"]
        }
    ],
    "virtual_link":[
        {
            "name":"new-network-jas"
        }
    ],
    "deployment_flavour":[
        {
            "flavour_key":"m1.small"
        }
    ],
    "type":"server",
    "endpoint":"docker"
}
