{
  "name": "oai-hss-cassandra",
  "vendor": "YB",
  "version": "docker",
  "vld": [{
    "name": "prod-oai-hss-cassandra-net"
  }],
  "vnfd": [
    {
      "name":"oai-hss",
      "vendor":"FOKUS",
      "version":"docker",
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "vdu":[
        {
          "vm_image":["ybello02/oai-hss-latest:latest"],
          "scale_in_out":3,
          "vnfc":[
        {
          "connection_point":[
            {
              "virtual_link_reference":"prod-oai-hss-cassandra-net"
            }
          ]
        }
          ]
        }
      ],
      "virtual_link":[
        {
           "name":"prod-oai-hss-cassandra-net"
        }
      ],
      "type":"server",
      "endpoint":"docker"
    },
    {
      "name":"oai-cassandra",
      "vendor":"YB",
      "version":"docker",
      "vdu":[
        {
          "vm_image":["ybello02/cassandra-latest:latest"],
          "scale_in_out":3,
          "vnfc":[
        {
          "connection_point":[
            {
              "virtual_link_reference":"prod-oai-hss-cassandra-net"
            }
          ]
        }
          ]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "virtual_link":[
        {
           "name":"prod-oai-hss-cassandra-net"
        }
      ],
      "requires": {
        "oai-hss": {
            "parameters":["hostname"]
      	}
      },
      "type":"server",
      "endpoint":"docker"
    }
  ]
}
